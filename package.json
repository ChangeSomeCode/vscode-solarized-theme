            },
            {
                "label": "ItCase-Solarized Light",
                "uiTheme": "vs",
                "path": "./themes/light-color-theme-itcase.json"
            }
        ]
    },
    "dependencies": {
        "@vscode/vsce": "^3.1.0"
    }

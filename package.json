{
    "name": "itcase-solarized",
    "version": "2.1.2",
    "displayName": "ItCase-Solarized",
    "description": "Optimized Solarized theme for VS Code",
    "publisher": "ChangeSomeCode",
    "repository": "https://github.com/ChangeSomeCode/vscode-solarized-theme",
    "license": "MIT",
    "icon": "icon.png",
    "keywords": [
        "solarized",
        "dark",
        "light"
    ],
    "engines": {
        "vscode": "^1.93.1"
    },
    "categories": [
        "Themes"
    ],
    "contributes": {
        "themes": [
            {
                "label": "ItCase-Solarized Dark",
                "uiTheme": "vs-dark",
